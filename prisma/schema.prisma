generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}



enum Role {
  ADMIN
  PROVIDER
  CUSTOMER
}

enum UStatus {
  SUSPEND
  ACTIVATE
}


model User {
    id        String   @id @default(uuid())
    email     String   @unique
    password  String
    name      String
    role      Role     @default(CUSTOMER)
    status    UStatus  @default(ACTIVATE)

    @@map("users")
}
